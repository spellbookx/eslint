{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "version": "independent",
  "npmClient": "pnpm",
  "command": {
    "version": {
      "allowBranch": "main",
      "message": "chore(release): publish %s",
      "conventionalCommits": true,
      "push": true,
      "changelog": false,
      "yes": true
    },
    "publish": {
      "yes": true
    }
  }
}
